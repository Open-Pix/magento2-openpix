{
  "name": "magento2-openpix",
  "version": "1.0.29",
  "scripts": {
    "es": "node -r esbuild-register",
    "op:vite": "vite checkout-success",
    "prepare": "husky install",
    "prettier": "prettier --write",
    "prettier-php": "prettier Pix/** --write",
    "release:major": "yarn es ./scripts/changelog --major",
    "release:minor": "yarn es ./scripts/changelog --minor",
    "release:patch": "yarn es ./scripts/changelog --patch",
    "release:zip": "bash ./pack.sh"
  },
  "dependencies": {
    "jest": "^27.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@octokit/rest": "^18.12.0",
    "@prettier/plugin-php": "^0.18.4",
    "@types/dotenv-safe": "^8.1.2",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "babel-jest": "^27.5.1",
    "dotenv-safe": "^8.2.0",
    "esbuild-register": "^2.5.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-typescript": "0.14.0",
    "generate-changelog": "^1.8.0",
    "husky": "^7.0.4",
    "lint-staged": "12.3.7",
    "moment": "^2.29.1",
    "prettier": "^2.6.1",
    "simple-git": "^3.5.0",
    "vite": "2.8.6"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.php": [
      "prettier --write"
    ]
  }
}
